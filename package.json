{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "start": "nest start",
    "start:dev": "nest start --watch",
    "build:all": "nest build api && nest build user",
    "///": "-------------[Start Production Commands]--------------",
    "start:prod": "node dist/apps/api/main",
    "start:prod:game": "node dist/apps/game/main",
    "//": "-------------------------------------------------------",
    "start:debug": "nest start --debug --watch",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/backend/test/jest-e2e.json",
    "proto:generate:game": "protoc --ts_out=./libs/common/src/proto-generated -I=./apps/game/src/proto"
  },
  "dependencies": {
    "@blazity/nest-file-fastify": "^1.0.0",
    "@fastify/cookie": "^9.4.0",
    "@fastify/csrf-protection": "^6.4.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "7.4.1",
    "@fastify/secure-session": "^7.5.1",
    "@fastify/static": "^7.0.4",
    "@maxmind/geoip2-node": "^5.0.0",
    "@nestjs/bullmq": "^10.2.0",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^10.4.1",
    "@nestjs/mongoose": "^10.0.6",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-fastify": "^10.3.8",
    "@nestjs/schedule": "^4.1.1",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/throttler": "^5.1.2",
    "@tma.js/init-data-node": "^1.2.0",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "big.js": "^6.2.1",
    "bullmq": "^5.12.7",
    "cache-manager": "^5.7.2",
    "cache-manager-redis-yet": "^5.1.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.11",
    "dotenv": "^16.4.5",
    "mongoose": "^8.3.4",
    "ms": "^2.1.3",
    "nanoid": "3",
    "nats": "^2.28.2",
    "nestjs-real-ip": "^3.0.1",
    "passport-custom": "^1.1.1",
    "protoc-gen-ts": "^0.8.7",
    "redis": "^4.6.14",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.11",
    "@types/async-retry": "^1.4.9",
    "@types/big.js": "^6.2.2",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.12.11",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "resolutions": {
    "wrap-ansi": "7.0.0",
    "string-width": "4.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s?$": [
        "@swc/jest"
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/core(|/.*)$": "<rootDir>/libs/core/src/$1",
      "^@app/app-services(|/.*)$": "<rootDir>/libs/app-services/src/$1",
      "^@app/middlewares(|/.*)$": "<rootDir>/libs/middlewares/src/$1",
      "^@app/interceptors(|/.*)$": "<rootDir>/libs/interceptors/src/$1",
      "^@app/integrations(|/.*)$": "<rootDir>/libs/integrations/src/$1"
    }
  }
}
